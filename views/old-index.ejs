<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>NODE JS</title>
    <link rel="stylesheet" type="text/css" href="/css/style.css">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:300' rel='stylesheet' type='text/css'>
  </head>
  <body>
    <div class="container">
      <table class="airtable_data">
        <tr>
          <th>Title</th>
          <th>Image</th>
          <th>Description</th>
          <th>Content Type</th>
          <th>City</th>
          <th>Focus Area</th>
          <th>Impact</th>
        </tr>

      <% for(var i=0; i<desclist.length; i++) {%>

         <tr>
           <td>
             <%= namelist[i]%>
           </td>
           <td>
             <%= image[i]%>
           </td>
           <td>
             <%= desclist[i]%>
           </td>
           <td>
             <%= ctype[i]%>
           </td>
           <td>
             <%= cities[i]%>
           </td>
           <td>
             <%= focus_area[i]%>
           </td>
           <td>
             <%= impact[i]%>
           </td>
         </tr>

      <% } %>

    </table>
    </div>
  </body>
</html>